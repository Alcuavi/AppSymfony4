{%  extends 'base.html.twig' %}
{% block body %}
   <table class="table">
      <thead>
      <tr>
         <th scope="col">#</th>
         <th scope="col">First</th>
         <th scope="col">Last</th>
         <th scope="col">Handle</th>
      </tr>
      </thead>
      <tbody>
      {% for equipo in equipos %}
      <tr>
         <th scope="row">{{ equipo.id }}</th>
         <td>{{ equipo.Categoria }}</td>
         <td>{{ equipo.sexo }}</td>
         <td>{{ equipo.numjugadores }}</td>
      </tr>
      {% endfor %}
      </tbody>
   </table>
{% endblock %}